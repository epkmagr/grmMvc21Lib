<!DOCTYPE html>
<html>
    <head>
        {% block head %}
            <meta charset="UTF-8">
            <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css" rel="stylesheet">
            <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap-theme.min.css"    rel="stylesheet">
            {% block stylesheets %}
                {{ encore_entry_link_tags('app') }}
            {% endblock %}
            <title>{% block title %}{% endblock %} - MVC webpage</title>
            <link rel="shortcut icon" href="{{ absolute_url('images/favicon.ico') }}">
        {% endblock %}


        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
{#     <body>
        {% block body %}{% endblock %}
    </body> #}
        <body role="document">
        <nav class="navbar navbar-inverse navbar-fixed-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false"   aria-controls="navbar">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="{{ path('me') }}">MVC</a>
                </div>
                <div id="navbar" class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">
                        <li><a href="{{ path('initImg') }}">Kmom03</a></li>
                        <li><a href="{{ path('app_book') }}">Kmom05</a></li>
                        <li><a href="{{ path('metrics') }}">metrics</a></li>
                    </ul>
                </div>
            </div>
        </nav>
        <div id="content">
            <div class="row">
                 <div class="col-md-10 col-md-offset-1">
                    {% block content %}{% endblock %}
                </div>
            </div>
        </div>
        <div id="footer" class="well text-center navbar-fixed-bottom my-footer">
            <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
            <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
            {% block footer %}
                &copy; 2023 Marie's programming Ltd.
            {% endblock %}
        </div>
    </body>
</html>
